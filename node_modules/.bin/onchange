#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/liam/devel/htmc/node_modules/.pnpm/onchange@7.1.0/node_modules/onchange/dist/node_modules:/home/liam/devel/htmc/node_modules/.pnpm/onchange@7.1.0/node_modules/onchange/node_modules:/home/liam/devel/htmc/node_modules/.pnpm/onchange@7.1.0/node_modules:/home/liam/devel/htmc/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/liam/devel/htmc/node_modules/.pnpm/onchange@7.1.0/node_modules/onchange/dist/node_modules:/home/liam/devel/htmc/node_modules/.pnpm/onchange@7.1.0/node_modules/onchange/node_modules:/home/liam/devel/htmc/node_modules/.pnpm/onchange@7.1.0/node_modules:/home/liam/devel/htmc/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/onchange@7.1.0/node_modules/onchange/dist/bin.js" "$@"
else
  exec node  "$basedir/../.pnpm/onchange@7.1.0/node_modules/onchange/dist/bin.js" "$@"
fi
